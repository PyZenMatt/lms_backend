{
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "lint:strict": "eslint 'src/**/*.{ts,tsx,js,jsx}' --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "typecheck:e2e": "tsc -p tsconfig.e2e.json --noEmit",
    "e2e:ci": "npm run typecheck:e2e && playwright test --reporter=html",
    "e2e:report": "playwright show-report",
    "tokens:check": "node scripts/tokens-check.mjs",
    "tokens:replace": "node scripts/tokenize-css.js",
    "openapi:gen": "openapi-typescript openapi/openapi.json -o src/lib/api/schema.ts",
    "tokens:apply": "node scripts/tokenize-css.mjs --root src --mapping scripts/color-mapping.js --ext css,scss,js,jsx,ts,tsx --write --report .tokenize-report.json",
    "imports:strip-versions": "node scripts/strip-import-versions.mjs",
    "imports:normalize": "node scripts/normalize-imports.mjs",
    "imports:dedupe-ui": "node scripts/dedupe-ui-imports.mjs",
    "ban:legacy": "node scripts/ban-legacy.mjs",
    "migrate:rb": "node scripts/migrate-react-bootstrap-imports.mjs",
    "purge:css": "node scripts/purge-css.mjs",
    "codemod:rb": "node scripts/codemod-react-bootstrap.mjs",
    "migrate:classes": "node scripts/migrate-bootstrap-classes.mjs",
    "cutover:v2": "npm run purge:css && npm run codemod:rb && npm run migrate:classes && npm run ban:legacy && npm run tokens:check",
    "cutover:imports": "npm run imports:normalize && npm run imports:dedupe-ui && npm run ban:legacy && npm run typecheck",
    "smoke:routes": "vitest run --reporter=dot",
    "lint:imports": "eslint 'src/**/*.{ts,tsx,jsx,js}' --max-warnings=0",
    "ban:jsx": "test -z \"$(find src -type f -name '*.jsx')\" || (echo '‚ùå Found .jsx files. Convert to .tsx'; exit 1)",
    "guard:ui": "npm run ban:legacy && npm run ban:jsx && npm run lint:imports && npm run typecheck && npm run build"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{md,json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "glob": "^11.0.3",
    "globby": "^14.1.0",
    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.9.1",
    "sass-embedded": "^1.90.0",
  "tailwindcss": "^4.0.0-alpha.19",
  "@tailwindcss/forms": "^0.5.9",
  "@tailwindcss/typography": "^0.5.15",
  "tailwindcss-animate": "^1.0.7",
  "autoprefixer": "^10.4.20",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-jsconfig-paths": "^2.0.1",
    "vitest": "^3.2.4",
    "web-vitals": "^2.1.4"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@mui/icons-material": "^7.3.1",
