{
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "typecheck:e2e": "tsc -p tsconfig.e2e.json --noEmit",
    "e2e:ci": "npm run typecheck:e2e && playwright test --reporter=html",
  "e2e:report": "playwright show-report",
  "tokens:check": "bash -c '! rg -n \"#[0-9a-fA-F]{3,6}|rgb\\(|rgba\\(\" src | tee /dev/stderr'",
  "openapi:gen": "openapi-typescript openapi/openapi.json -o src/lib/api/schema.ts"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{md,json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-react": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "openapi-typescript": "^7.9.1",
    "sass-embedded": "^1.90.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-jsconfig-paths": "^2.0.1"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@stripe/react-stripe-js": "^3.9.0",
    "@stripe/stripe-js": "^7.8.0",
    "@tanem/react-nprogress": "^5.0.55",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.7",
    "date-fns": "^4.1.0",
    "ethers": "^6.15.0",
    "formik": "^2.4.6",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-bootstrap-icons": "^1.11.6",
    "react-dom": "^19.1.1",
    "react-helmet": "^6.1.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-router-dom": "^7.8.1",
    "terser": "^5.43.1",
    "web-vitals": "^5.1.0",
    "yup": "^1.7.0"
  }
}
