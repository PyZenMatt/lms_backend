{
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "typecheck:e2e": "tsc -p tsconfig.e2e.json --noEmit",
    "e2e:ci": "npm run typecheck:e2e && playwright test --reporter=html",
    "e2e:report": "playwright show-report",
    "tokens:check": "bash -c '! rg -n \"#[0-9a-fA-F]{3,6}|rgb\\(|rgba\\(\" src | tee /dev/stderr'",
    "tokens:replace": "node scripts/tokenize-css.js",
    "openapi:gen": "openapi-typescript openapi/openapi.json -o src/lib/api/schema.ts",
    "tokens:apply": "node scripts/tokenize-css.mjs --root src --mapping scripts/color-mapping.js --ext css,scss,js,jsx,ts,tsx --write --report .tokenize-report.json",
    "imports:strip-versions": "node scripts/strip-import-versions.mjs"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{md,json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-react": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "glob": "^11.0.3",
    "globby": "^14.1.0",
    "openapi-typescript": "^7.9.1",
    "sass-embedded": "^1.90.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vite-jsconfig-paths": "^2.0.1"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
